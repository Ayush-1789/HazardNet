# HazardNet Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control!

# Backend API Configuration
API_BASE_URL=https://your-backend-api.com/api/v1
API_TIMEOUT_SECONDS=30

# Database Configuration (for backend - not used in Flutter frontend)
# These are examples if you need to document backend setup
# DATABASE_URL=postgresql://user:password@localhost:5432/hazardnet
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_DB=hazardnet
# POSTGRES_USER=your_user
# POSTGRES_PASSWORD=your_password

# Authentication
# JWT_SECRET=your-jwt-secret-here
# JWT_EXPIRY_HOURS=24

# Google Maps API Key (if using Maps SDK)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Optional: Third-party services
# SENTRY_DSN=your_sentry_dsn_for_error_tracking
# ANALYTICS_KEY=your_analytics_key

# Feature Flags (optional)
ENABLE_MOCK_AUTH=true
ENABLE_DEBUG_LOGGING=false
